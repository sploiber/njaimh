<h2><%= @user %></h2>
<%= link_to "Edit User", edit_admin_user_path(@user), :class => "btn btn-primary btn-large" %>
<%= link_to "Delete User", admin_user_path(@user), :method => :delete,
  :confirm => "Are you sure you want to delete this user?", :class => "btn btn-primary btn-large" %>
<%= link_to "Return to all users", admin_users_path, :class => "btn btn-primary btn-large" %>
